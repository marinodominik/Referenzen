COACHSUCCESS  = $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH1)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH2)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH3)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH4)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH5)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGCOACHLENGTHLEFT))"$(COACH6)"$(shell printf '%*s' $(REMAININGCOACHLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHLEFT))"$(SUCCESS1)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHLEFT))"$(SUCCESS2)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHLEFT))"$(SUCCESS3)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHLEFT))"$(SUCCESS4)"$(shell printf '%*s' $(REMAININGSUCCESSLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHLEFT))"$(GENERATED1)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHLEFT))"$(GENERATED2)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHLEFT))"$(GENERATED3)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHRIGHT))"$(BOXLINEEND) $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGGENERATEDLENGTHLEFT))"$(GENERATED4)"$(shell