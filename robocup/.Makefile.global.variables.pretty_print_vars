OUTPUTLENGTH = $(shell tput cols)

TARGETNAME   = $(shell basename $@)

ENTER   = " Enter Directory: "
LEAVE   = " Leave Directory: "
COMPILE = " Compile : "
LINK    = " LINK : "
PACK    = " Pack Library : "
DONE    = " DONE! "

BOXLINESTART      = "|"
BOXLINEEND        = "|"
BOXDOUBLEFILLCHAR = "="
BOXFILLCHAR       = "-"
BOXEMPTYFILLCHAR  = " "
DONEFILLCHAR      = "."

TARGETLENGTH       = $(shell echo -n $(TARGETNAME)     | wc -c )
ACTDIRLENGTH       = $(shell echo -n $(ACTDIRRELATIVE) | wc -c )
ENTERLENGTH        = $(shell echo -n $(ENTER)          | wc -c )
LEAVELENGTH        = $(shell echo -n $(LEAVE)          | wc -c )
COMPILELENGTH      = $(shell echo -n $(COMPILE)        | wc -c )
LINKLENGTH         = $(shell echo -n $(LINK)           | wc -c )
PACKLENGTH         = $(shell echo -n $(PACK)           | wc -c )
DONELENGTH         = $(shell echo -n $(DONE)           | wc -c )
BOXLINESTARTLENGTH = $(shell echo -n $(BOXLINESTART)   | wc -c )
BOXLINEENDLENGTH   = $(shell echo -n $(BOXLINEEND)     | wc -c )

REMAININGLINELENGTH          = $(shell expr $(OUTPUTLENGTH) - $(BOXLINESTARTLENGTH) - $(BOXLINEENDLENGTH))

REMAININGENTERACTDIRLENGTH   = $(shell expr $(REMAININGLINELENGTH) - $(ACTDIRLENGTH) - $(ENTERLENGTH))
REMAININGLEAVEACTDIRLENGTH   = $(shell expr $(REMAININGLINELENGTH) - $(ACTDIRLENGTH) - $(LEAVELENGTH))
REMAININGCOMPILETARGETLENGTH = $(shell expr $(REMAININGLINELENGTH) - $(COMPILELENGTH) - $(TARGETLENGTH) - $(DONELENGTH))
REMAININGLINKTARGETLENGTH    = $(shell expr $(REMAININGLINELENGTH) - $(LINKLENGTH)    - $(TARGETLENGTH) - $(DONELENGTH))
REMAININGPACKLIBLENGTH       = $(shell expr $(REMAININGLINELENGTH) - $(PACKLENGTH)    - $(TARGETLENGTH) - $(DONELENGTH))

BOXDOUBLESEPERATOR = $(BOXLINESTART)$(shell printf '%0.1s' $(BOXDOUBLEFILLCHAR){1..$(REMAININGLINELENGTH)})$(BOXLINEEND)
BOXSEPERATOR       = $(BOXLINESTART)$(shell printf '%0.1s' $(BOXFILLCHAR){1..$(REMAININGLINELENGTH)})$(BOXLINEEND)
BOXEMPTYSEPERATOR  = $(BOXLINESTART)"$(shell printf '%*s' $(REMAININGLINELENGTH))"$(BOXLINEEND)

ENTER_DIRECTORY = $(BOXLINESTART)$(ENTER)$(ACTDIRRELATIVE)"$(shell printf '%*s' $(REMAININGENTERACTDIRLENGTH))"$(BOXLINEEND)
LEAVE_DIRECTORY = $(BOXLINESTART)$(LEAVE)$(ACTDIRRELATIVE)"$(shell printf '%*s' $(REMAININGLEAVEACTDIRLENGTH))"$(BOXLINEEND)

COMPILETARGET = $(BOXLINESTART)$(COMPILE)$(TARGETNAME)$
COMPILETARGETDONE = $(shell printf '%0.1s' $(DONEFILLCHAR){1..$(REMAININGCOMPILETARGETLENGTH)})$(DONE)$(BOXLINEEND)

LINKTARGET = $(BOXLINESTART)$(LINK)$(TARGETNAME)$
LINKTARGETDONE = $(shell printf '%0.1s' $(DONEFILLCHAR){1..$(REMAININGLINKTARGETLENGTH)})$(DONE)$(BOXLINEEND)

PACKLIB = $(BOXLINESTART)$(PACK)$(TARGETNAME)$
PACKLIBDONE = $(shell printf '%0.1s' $(DONEFILLCHAR){1..$(REMAININGPACKLIBLENGTH)})$(DONE)$(BOXLINEEND)

PRINT    = @printf '%s'
PRINTLN  = @printf '%s\n'
PRINT2LN = @printf '%s\n%s'

PRINT_BOXDOUBLESEPERATOR = $(PRINTLN)  $(BOXDOUBLESEPERATOR)
PRINT_BOXSEPERATOR       = $(PRINTLN)  $(BOXSEPERATOR)
PRINT_BOXEMPTYSEPERATOR  = $(PRINTLN)  $(BOXEMPTYSEPERATOR)
ifeq ($(PLAIN_MODE), 0)
PRINT_ENTER_DIRECTORY    = $(PRINTLN)  $(BOXDOUBLESEPERATOR) $(ENTER_DIRECTORY) $(BOXSEPERATOR) $(BOXEMPTYSEPERATOR)
PRINT_LEAVE_DIRECTORY    = $(PRINTLN)  $(BOXEMPTYSEPERATOR) $(BOXSEPERATOR) $(LEAVE_DIRECTORY) $(BOXDOUBLESEPERATOR)
PRINT_COMPILETARGET      = $(PRINT)    $(COMPILETARGET)
PRINT_COMPILETARGETDONE  = $(PRINTLN)  $(COMPILETARGETDONE)
PRINT_LINKTARGET         = $(PRINT2LN) $(BOXEMPTYSEPERATOR) $(LINKTARGET)
PRINT_LINKTARGETDONE     = $(PRINTLN)  $(LINKTARGETDONE)
PRINT_PACKLIB            = $(PRINT2LN) $(BOXSEPERATOR) $(PACKLIB)
PRINT_PACKLIBDONE        = $(PRINTLN)  $(PACKLIBDONE) $(BOXSEPERATOR)

PRINT_CLEAN              = $(PRINTLN) " " " ===> "$(ACTDIRRELATIVE)" is now clean! <=== " " "
else
PRINT_ENTER_DIRECTORY    = @:
PRINT_LEAVE_DIRECTORY    = @:
PRINT_COMPILETARGET      = @:
PRINT_COMPILETARGETDONE  = @:
PRINT_LINKTARGET         = @:
PRINT_LINKTARGETDONE     = @:
PRINT_PACKLIB            = @:
PRINT_PACKLIBDONE        = @:

PRINT_CLEAN              = @:
endif


AGENT1 = "      _       _____   ______   _   _   _______  "
AGENT2 = "     / \     / ____| |  ____| | \ | | |__   __| "
