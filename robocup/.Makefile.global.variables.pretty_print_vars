GENERATELENGTH  = $(shell echo -n $(GENERATE1)  | wc -c )
ALLDONELENGTH   = $(shell echo -n $(ALLDONE1)   | wc -c )

REMAININGAGENTLENGTH     = $(shell expr $(REMAININGLINELENGTH) - $(AGENTLENGTH)     )
REMAININGCOACHLENGTH     = $(shell expr $(REMAININGLINELENGTH) - $(COACHLENGTH)     )
REMAININGLIBLENGTH       = $(shell expr $(REMAININGLINELENGTH) - $(LIBLENGTH)       )
REMAININGSUCCESSLENGTH   = $(shell expr $(REMAININGLINELENGTH) - $(SUCCESSLENGTH)   )
REMAININGGENERATEDLENGTH = $(shell expr $(REMAININGLINELENGTH) - $(GENERATEDLENGTH) )
REMAININGGENERATELENGTH  = $(shell expr $(REMAININGLINELENGTH) - $(GENERATELENGTH)  )
REMAININGALLDONELENGTH   = $(shell expr $(REMAININGLINELENGTH) - $(ALLDONELENGTH)  )

REMAININGAGENTLENGTHLEFT     = $(shell expr $(REMAININGAGENTLENGTH)     / 2 )
REMAININGCOACHLENGTHLEFT     = $(shell expr $(REMAININGCOACHLENGTH)     / 2 )
REMAININGLIBLENGTHLEFT       = $(shell expr $(REMAININGLIBLENGTH)       / 2 )
REMAININGSUCCESSLENGTHLEFT   = $(shell expr $(REMAININGSUCCESSLENGTH)   / 2 )
REMAININGGENERATEDLENGTHLEFT = $(shell expr $(REMAININGGENERATEDLENGTH) / 2 )
REMAININGGENERATELENGTHLEFT  = $(shell expr $(REMAININGGENERATELENGTH)  / 2 )
REMAININGALLDONELENGTHLEFT   = $(shell expr $(REMAININGALLDONELENGTH)   / 2 )

REMAININGAGENTLENGTHRIGHT     = $(shell expr $(REMAININGAGENTLENGTH)     - $(REMAININGAGENTLENGTHLEFT)     )
REMAININGCOACHLENGTHRIGHT     = $(shell expr $(REMAININGCOACHLENGTH)     - $(REMAININGCOACHLENGTHLEFT)     )
REMAININGLIBLENGTHRIGHT       = $(shell expr $(REMAININGLIBLENGTH)       - $(REMAININGLIBLENGTHLEFT)       )
REMAININGSUCCESSLENGTHRIGHT   = $(shell expr $(REMAININGSUCCESSLENGTH)   - $(REMAININGSUCCESSLENGTHLEFT)   )
REMAININGGENERATEDLENGTHRIGHT = $(shell expr $(REMAININGGENERATEDLENGTH) - $(REMAININGGENERATEDLENGTHLEFT) )
REMAININGGENERATELENGTHRIGHT  = $(shell expr $(REMAININGGENERATELENGTH)  - $(REMAININGGENERATELENGTHLEFT)  )
REMAININGALLDONELENGTHRIGHT   = $(shell expr $(REMAININGALLDONELENGTH)   - $(REMAININGALLDONELENGTHLEFT)   )

